		@include respond-to(wide) {left:-47px;}
		&:before{
			right:-8px;
		}
	}
	.whyus7__r{
		right:0;
		&:before{
			left:-9px;
		}
	}
	.whyus7__icon{
		width:80px;
		height:80px;
		position:absolute;
		z-index:1;
		top:0;
		left:0;
		background:url(image-path('whyus7.png')) no-repeat 0 0;
		
	}
	.whyus7__1{
		.whyus7__icon{
			background-position:0 0;
			&:hover{
				background-position:0 100%;	
			}
		}
		.whyus7__l{display:none;}
	}
	.whyus7__2{
		.whyus7__icon{
			background-position:-144px 0;
			&:hover{
				background-position:-144px 100%;	
			}
		}
	}
	.whyus7__3{
		.whyus7__icon{
			background-position:-288px 0;
			&:hover{
				background-position:-288px 100%;	
			}
		}
	}
	.whyus7__4{
		.whyus7__icon{
			background-position:-433px 0;
			&:hover{
				background-position:-433px 100%;	
			}
		}
	}
	.whyus7__5{
		.whyus7__icon{
			background-position:-578px 0;
			&:hover{
				background-position:-578px 100%;	
			}
		}
	}
	.whyus7__6{
		.whyus7__icon{
			background-position:-723px 0;
			&:hover{
				background-position:-723px 100%;	
			}
		}
	}
	.whyus7__7{
		margin-right:0;
