.promo__holder{
	border-top:1px solid #b8b8b8;
	.promo_carusel{
		position:relative;
		top:-1px;
	}
}

.promo{
	position:relative;
	.promo__wrap{
		overflow:hidden;
		height:300px;
		
		position:relative;
	}
	.promo__nav{
		position:absolute;
		width:26px;
		right:-26px;
		top:0px;
		background:#fff;
		
		a{
			display:block;
			margin-left:10px;
			margin-bottom:14px;
			font-size:0px;
			width:4px;
			height:4px;
			border:1px solid #c6c6c6;
		}
		.promo__active,.active{
			background:#e5233a;
			border-color:#e5233a;
		}
	}
	.promo__slides{
		position:absolute;
		left:0;
		top:0;
		width:990px;
		@include respond-to(wide) {width:1180px;}
		height:20000em;
	}
	.promo__slide{
		float:left;
		overflow:hidden;
		position:relative;
		height:300px;
		width:990px;
		@include respond-to(wide) {width:1180px;}
	}
	.promo__pic{
		width:100%;
		img{
			width:100%;
		}
	}
	.promo__content{
		position:absolute;
		left:510px;
		top:55px;
		width:480px;
		@include respond-to(wide) {left:605px;}
	}
	h2{
		font-size:60px;
		font-weight:normal;
		color:#000;
		font-family:Georgia,serif;
	}
	h3{
		font-size:20px;
		color:#363b3b;
		font-weight:normal;
		font-family:Georgia,serif;
		margin-bottom:23px;
	}
	p{
		padding-top:24px;
		position:relative;
		text-transform:uppercase;
		font-size:12px;
		&:before{
			content:' ';
			display:block;
			width:30px;
			border-top:3px solid #000;
			position:absolute;
			top:0;
			left:0;
		}
		a{
			color:#000;
			margin-right:23px;
			font-weight:bold;
		}
	}
	img{
		display:block;
		max-width:100%;
	}
}
.promos{
	width:100%;
	overflow:hidden;
	margin:30px 0;
	padding-bottom:30px;
