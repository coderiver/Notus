.lookevent{
	border-bottom:4px solid #000;
	margin-bottom:22px;
	.lookevent__carowrap{
		border-top:1px solid #e5e5e5;
		position:relative;
		width:100%;
		&:before{
			top:0;
			content:' ';	
			position:absolute;
			width:400px;
			background:rgba(255,255,255,0.8);
			height:503px+14px;
			@include respond-to(wide) {height:560px+14px;}
			left:-400px;
			z-index:20
		}
		&:after{
			content:' ';	
			position:absolute;
			width:400px;
			background:rgba(255,255,255,0.8);
			height:503px+14px;
			@include respond-to(wide) {height:560px+14px;}
			right:-400px;
			bottom:0;
			z-index:20
		}
	}
	.h2{
		margin-bottom:18px;
		a{
			color:#000;
		}
	}
	.item{
		float:left;
		height:503px;
		padding-top:14px;
		position:relative;
		border-bottom:none;
		@include respond-to(wide) {height:560px;}
	}
	.lookevent__prev,.lookevent__next{
		position:absolute;
		top:100px;
		width:20px;
		height:100px;
		cursor:pointer;
		z-index:200;
		top:159px;
		top:0;
		bottom:0;
		
		
		width:30px;
		height:117px;
		height:100%;
		height:503px+14px;
		@include respond-to(wide) {height:560px+14px;}
	}
	.lookevent__prev{left:-27px;background: url(image-path('le-prev.png')) no-repeat 50% 50%;}
	.lookevent__next{right:-29px;background: url(image-path('le-next.png')) no-repeat 50% 50%;}
	.lookevent__caro{
		width:100%;
		height:503px+14px;
		@include respond-to(wide) {height:560px+14px;}
		position:relative;
	}
	.lookevent__slides{
		width:2000em;
		position:absolute;
		margin-left:-11px;
	}
}
